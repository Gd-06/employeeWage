package Day7;

public class empWageUC1 {

    public static void main(String[] args) {


        int fullday = 1;
        double cal = Math.floor(Math.random() * 10) % 2;

        if (cal== fullday) {
            System.out.println("Employee is Present");
        } else {
            System.out.println("Employee is Absent");
        }


    }
}

